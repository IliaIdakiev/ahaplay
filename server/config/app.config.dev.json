{
  "port": 8000,
  "webAppUrl": "localhost:4200",
  "serverUrl": "localhost:8000",
  "cookieSecret": "very-well-kept-secret",
  "jwt": {
    "secret": "very-well-kept-token-secret",
    "issuer": "ahaplay",
    "accessTokenExpiryTime": "1h",
    "refreshTokenExpiryTime": "1d"
  },
  "email": {
    "disableEmails": true,
    "sendgridApiKey": "",
    "sendgridTemplateIds": {
      "authorChallenge": "",
      "emailConfirmation": ""
    },
    "defaults": {
      "noReplyEmail": "noreply@ahaplay.com",
      "requestsEmail": "requests@ahaplay.com"
    }
  },
  "https": {
    "cert": null,
    "privKey": null
  },
  "authCookieName": "auth",
  "authHeaderName": "Authorization",
  "refreshTokenName": "refresh",
  "hashSaltRounds": 10,
  "allowGraphqlSandbox": true,
  "sessionProcessorDebugPort": "9229",
  "syncSequelizeModels": true,
  "certificateLocation": "/etc/ssl/certs",
  "splitWaitingTime": "5m"
}